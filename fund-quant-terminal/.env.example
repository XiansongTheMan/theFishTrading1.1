# =====================================================
# 复制为 .env 供 docker compose 使用
# cp .env.example .env && 根据需要修改
# =====================================================

# MongoDB（容器内使用服务名 mongodb）
MONGODB_URL=mongodb://mongodb:27017
MONGODB_DB_NAME=fund_quant

# Tushare（可选）
TUSHARE_TOKEN=

# 应用
APP_DEBUG=true
APP_HOST=0.0.0.0
APP_PORT=8000

# CORS
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
